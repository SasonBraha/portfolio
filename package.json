{
  "name": "my-personal-website",
  "version": "1.0.0",
  "description": "My Personal Website (Portfolio)",
  "main": "dist/index.html",
  "scripts": {
    "dev": "concurrently \"set NODE_ENV=development&& nodemon server/server.js\" \"webpack-dev-server --mode development --open\"",
    "build": "rm -rf dist && webpack --mode production",
    "deploy:client-stage": "rm -rf dist && webpack --mode production && netlify deploy --dir=dist",
    "deploy:client": "rm -rf dist && webpack --mode production && netlify deploy -p --dir=dist",
    "deploy:server": "cd server && git push heroku master"
  },
  "author": "Sason Braha",
  "license": "ISC",
  "homepage": "https://sasonbraha.com",
  "devDependencies": {
    "@types/node": "^10.12.7",
    "autoprefixer": "^8.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-loader-minify-class": "^1.3.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^7.0.2",
    "rename-css-selectors": "^3.1.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {}
}
